priority 1

snippet { "Variable" i
{{ ${1:variable} }}$0
endsnippet

snippet help "Helper" i
{{${1:helper} ${2:${VISUAL:params}} }}$0
endsnippet

snippet raw "Raw variable" i
{{{ ${1:variable} }}}$0
endsnippet

snippet # "Section" i
{{#$1}}
${2:${VISUAL:body}}
{{/$1}}$0
endsnippet

snippet ^ "Inverted section" i
{{^$1}}
${2:${VISUAL:body}}
{{/$1}}$0
endsnippet

snippet > "Partial" i
{{> $1}}$0
endsnippet

snippet ! "Comment" i
{{! ${1:${VISUAL:body}}${2:\}\}}$0
endsnippet
