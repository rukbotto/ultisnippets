priority 1

snippet {} "Block"
{
	${1:${VISUAL}}
}$0
endsnippet

snippet include "Include"
@include $1;$0
endsnippet

snippet import "Import"
@import $1;$0
endsnippet

snippet call "Call mixin"
${1:mixin}${2:($3)};$0
endsnippet

snippet mixin "Define mixin"
${1:mixin}${2:($3)} {
	${4:${VISUAL}}
}$0
endsnippet
