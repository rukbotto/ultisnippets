snippet "" "Quotes" i
"$1"$0
endsnippet

snippet '' "Single quotes" i
'$1'$0
endsnippet

snippet {} "Brackets" i
{$1}$0
endsnippet

snippet () "Round brackets" i
($1)$0
endsnippet

snippet [] "Square brackets" i
[$1]$0
endsnippet

snippet <> "Angular brackets" i
<$1>$0
endsnippet

snippet || "Straight brackets" i
|$1|$0
endsnippet

snippet `` "Backticks" i
\`$1\`$0
endsnippet

snippet // "Double slash" i
/$1/$0
endsnippet
